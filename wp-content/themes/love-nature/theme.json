{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#536942",
					"name": "Primary",
					"slug": "custom-primary"
				},
				{
					"color": "#f6f8f5",
					"name": "Secondary",
					"slug": "custom-secondary"
				},
				{
					"color": "#ffd936",
					"name": "Accent",
					"slug": "custom-accent"
				},
				{
					"color": "#000",
					"name": "Background",
					"slug": "custom-background"
				},
				{
					"color": "#ffffff",
					"name": "Text",
					"slug": "custom-text"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "250",
							"src": "file:./assets/fonts/DMSans-Thin.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": "file:./assets/fonts/DMSans-Black.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/DMSans-Bold.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": "file:./assets/fonts/DMSans-ExtraBold.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": "file:./assets/fonts/DMSans-Light.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": "file:./assets/fonts/DMSans-Medium.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/DMSans-Regular.ttf"
						},
						{
							"fontFamily": "\"DM Sans\"",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": "file:./assets/fonts/DMSans-SemiBold.ttf"
						}
					],
					"fontFamily": "\"DM Sans\"",
					"name": "DM Sans",
					"slug": "dm-sans"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--custom-background)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--custom-accent)",
					"text": "var(--wp--preset--color--custom-primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-background)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-sans)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json"
}